page {
  background: #eee;
}

.me {
  width: 750rpx;
  height: 161rpx;
  margin-bottom: 10px;
  position: relative;
}

.meImg {
  width: 100%;
  height: 100%;
}

.meBox {
  position: absolute;
  width: 750rpx;
  height: 161rpx;
  font-size: 24px;
  color: #333;
}

.head {
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
  margin-right: 40rpx;
}

.tabbox {
  height: 90rpx;
  color: #666;
  font-size: 28rpx;
  background: #fff;
}

.tabItem {
  width: 130rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
  height: 90rpx;
}

.active {
  color: #33BBAF;
  border-bottom: 2px solid #33BBAF;
}